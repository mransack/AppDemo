table.dataTable {
    clear: both;
    margin-top: 0px !important;
    margin-bottom: 6px !important;
    max-width: none !important;
    border-collapse: separate !important;
}

/*--data table customize css begin--*/

#example_wrapper .row:nth-of-type(1) {
    display: none;
}

#example_wrapper .row:nth-of-type(2) {
    overflow-x: hidden;
    max-width:100%;
    margin-left: 0px;
    margin-top:0px;
    position: relative;
    border-left: 1px solid #d6d6d6;
    padding-bottom: 1px;
    border-bottom: 1px solid #d6d6d6;
    border-right: 1px solid #d6d6d6;
}

